window.addEventListener('load', () => {
  const zipBtn = document.querySelector('[data-zp-add-to-cart]');
  // Create bubble element to inject

  const customUpdateCartBubble = (count) => {
    const parent = document.querySelector('#cart-icon-bubble');

    let childElement = document.createElement('div');
    childElement.className = 'cart-count-bubble';

    childElement.innerHTML = `
     <span aria-hidden="true" style="color: black;">${count}</span>
            <span class="visually-hidden">Cart</span>
  `;

    parent.appendChild(childElement);
  };

  zipBtn.addEventListener('click', () => {
    setTimeout(() => {
      console.log('Timeout logic starts here');
      const cartContents = fetch('/cart.js')
        .then((response) => response.json())
        .then((data) => { 
          const count = data.item_count;
          // Injection logic
          customUpdateCartBubble(count);
        });
    }, 1500);
  });
});

</script>

