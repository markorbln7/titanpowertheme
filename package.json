    "tweezer.js": "^1.5.0",
    "up-down": "0.0.1",
    "vue": "^2.5.17",
    "vue-click-outside": "^1.0.7",
    "vue-quick-search": "0.0.6",
    "vue-scrollto": "^2.13.0",
    "vuex": "^3.1.0",
    "vue-template-compiler": "^2.6.14",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}