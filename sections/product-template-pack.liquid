<div class="atc-fixed">
    <div class="atc-fixed__container">
        <div class="buds-product__inner--content__subtitle">
                    TITAN POWER CABLE
        </div>
        <button class="js-atc-scroll f aic jcc w100 bg-titan-green font-sans btn--add-to-cart color-white sticky-button">
                ORDER NOW ☝
        </button>
    </div>
</div>
{% schema %}
{
  "name": "Product Template",
  "settings": [
    {
      "type": "text",
      "id": "punchline",
      "label": "Punch Line",
      "default": "Finally! A charger that doesn't break."
    },
    {
      "type": "text",
      "id": "d1",
      "label": "Discount message 1",
      "default": "60% OFF + FREE CABLE TODAY!"
    },
    {
      "type": "text",
      "id": "d2",
      "label": "Discount message 2",
      "default": "60% OFF"
    },
    {
      "type": "text",
      "id": "d3",
      "label": "Discount message 3",
      "default": "50% OFF"
    },
    {
      "type": "text",
      "id": "idea",
      "label": "Pro Tip",
      "default": "This cable takes an incredible 1,500 lbs of force to break! Whilst allowing charging up to 2.4x faster!"
    }
  ],
  "blocks": [
    {
        "type": "bullets",
        "name": "Bullet Points",
        "settings": [
          {
            "type": "text",
            "id": "text",
            "label": "Text"
          }
        ]
      }
  ],
  "presets": [
    {
      "name": "Product Template",
      "category": "Home Page"
    }
  ]
}
{% endschema %}
