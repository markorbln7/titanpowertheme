{% assign product2 = all_products['1-x-titan-smart-cable™'] %}
{% assign product4 = all_products['2-x-titan-smart-cable™-2'] %}
{% assign plug = all_products['titan-45w-turbo-charge-plug™-50-off']%}
<section id="product-template tt" class="product-template titan-bg-black">
  <div class="container">
    <div class="product-template__wrapper f ais jcc">
        <div class="product-template__wrapper--image column-left">
            {% assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image %}
            <img class="js-main-image-change w-100" src="https://cdn.shopify.com/s/files/1/0071/1727/5191/files/titan_smart_cable_titan_power_plus_cord_charger_decoy_3-pack_iphone.jpg?v=1680171839" alt="{{ featured_image.alt }}">
        </div>
        <div class="product-template__wrapper--content column-right">
            <div class="product-template__wrapper--content__review js-section-scroll">
                <span class="product-template__wrapper--content__review--stars color-titan-green font-sans">★ ★ ★ ★ ★ </span>
                <span class="product-template__wrapper--content__review--mark font-sans color-white">4.7 </span>
                <span class="product-template__wrapper--content__review--number color-gray font-lato">(21,956)</span>
            </div>
              <h1 class="h1 product-template__wrapper--content__title color-white">
                 "{{section.settings.punchline}}"
              </h1>

              <div class="product-template__wrapper--content__features font-sans">
                  {% for block in section.blocks %}
                    {% if block.type == 'bullets'%}
                      <div class="product-template__wrapper--content__features--name">{{block.settings.text}}</div>
                    {% endif %}
                  {% endfor %}
              </div>
              <div class="product-template__wrapper--content__offerDiscount font-sans js-discount text-center">
                  {{section.settings.d1}}
              </div>

              <div class="relative">
                <div class="value-slider">
                  <div class="slider">
                      <div class="one-behind left two"><img class="w-100" src="https://cdn.shopify.com/s/files/1/0071/1727/5191/files/titan_smart_cable_titan_power_plus_cord_charger_decoy_2x_pack.jpg?v=1680169309"></div>
                      <div class="active three"><img class="w-100" src="https://cdn.shopify.com/s/files/1/0071/1727/5191/files/titan_smart_cable_titan_power_plus_cord_charger_decoy_3x_pack.jpg?v=1680169310"></div>
                      <div class="one-behind right one"><img class="w-100" src="https://cdn.shopify.com/s/files/1/0071/1727/5191/files/titan_smart_cable_titan_power_plus_cord_charger_decoy_1x_pack.jpg?v=1680169309"></div>
                  </div>
                </div>
              </div>
              <div class="product-template__wrapper--content__imageSelection">
                <img class="js-image-change d-block w-100" src="https://cdn.shopify.com/s/files/1/0071/1727/5191/files/Titan_smart_cable_titan_power_plus_iphone_1.2m_2.jpg?v=1680176374">
              </div>
              <div class="product-template__wrapper--content__selection">
                  <div class="product-template__wrapper--content__selection--wrapper f aic jcb fw">
                      <div data-discount="{{section.settings.d1}}" data-slide=".three" data-count="3x" data-id="{{product.id}}" data-product="3-x-titan-smart-cable™" class="master-product active js-product-selector product-template__wrapper--content__selection--wrapper__single f">
                          <div class="decoy-title">3x Pack</div>
                          <div class="decoy-content">
                            <div class="decoy-content__price js-price-discount-three"></div>
                            <div class="crossed titan-red bold js-price-original-three"></div>
                            <span class="product-template__wrapper--content__selection--wrapper__save">60% OFF<br><span class="free-cable">+ 1 FREE</span></span>
                          </div>
                      </div>
                      <div data-discount="{{section.settings.d2}}" data-slide=".two" data-count="2x" data-id="{{product4.id}}" data-product="2-x-titan-smart-cable™-2" class="js-product-selector product-template__wrapper--content__selection--wrapper__single f">
                         <div class="decoy-title">2x Pack</div>
                         <div class="decoy-content">
                          <div class="decoy-content__price js-price-discount-two"></div>
                          <div class="crossed titan-red bold js-price-original-two"></div>
                          <span class="product-template__wrapper--content__selection--wrapper__save">60% OFF</span>
                         </div>
                      </div>
                      <div data-discount="{{section.settings.d3}}" data-slide=".one" data-count="" data-id="{{product2.id}}" data-product="1-x-titan-smart-cable™" class="js-product-selector product-template__wrapper--content__selection--wrapper__single f">
                          <div class="decoy-title">1x Pack</div>
                          <div class="decoy-content">
                            <div class="decoy-content__price js-price-discount-one"></div>
                            <div class="crossed titan-red bold js-price-original-one"></div>
                            <span class="product-template__wrapper--content__selection--wrapper__save">50% OFF</span>
                          </div>
                      </div>
                  </div>
              </div>
              <div class="product-template__wrapper--content__selection small-top">
                  <div class="product-template__wrapper--content__selection--wrapper f aic jcb small-top fw">
                      <div data-type="iPhone" class="active js-type-selector product-template__wrapper--content__selection--wrapper__single">
                          <div>IPhone</div>
                      </div>
                      <div data-type="Type-C" class="js-type-selector product-template__wrapper--content__selection--wrapper__single">
                          <div>Type-C</div>
                      </div>
                      <div data-type="Micro-USB" class="js-type-selector product-template__wrapper--content__selection--wrapper__single">
                          <div>Micro Usb</div>
                      </div>
                  </div>
              </div>
              <div class="product-template__wrapper--content__selection">
                  <div class="product-template__wrapper--content__selection--wrapper f aic jcb small-top fw">
            
                      <div data-length="4ft(1.2m)" class="active js-length-selector product-template__wrapper--content__selection--wrapper__single">
                        1.2m
                      </div>
                      <div data-length="6ft(1.8m)" class="js-length-selector product-template__wrapper--content__selection--wrapper__single">
                        1.8m
                      </div>
                      <div data-length="10ft(3.0m)" class="js-length-selector product-template__wrapper--content__selection--wrapper__single">
                        3.0m
                      </div>
                  </div>
              </div>
              <div class="product-template__wrapper--content__add">
                  <div data-addon="{{plug.first_available_variant.id}}" class="f jcb aic product-template__wrapper--content__add--colored js-add">
                    <div class="product-template__wrapper--content__add--title font-bold">ADD TITAN PD PLUG <br>{{plug.price | money }} <span class="crossed titan-red">{{plug.compare_at_price | money }}</span></div>
                    <div class="product-template__wrapper--content__add--image">
                        {% assign featured_image_plug = plug.selected_or_first_available_variant.featured_image | default: plug.featured_image %}
                        <img class="w-100" src="{{ featured_image_plug | img_url: 'master' }}" alt="{{ featured_image_plug.alt }}">
                    </div>
                  </div>
              </div>
              <div class="text-center price-div">
                <span class="js-price titan-green">{{product.price | money}}</span> <span class="js-crossed-price titan-red">{{product.compare_at_price | money}}</span>
                <div class="js-saving">{{product.compare_at_price | minus:product.price | money}}</div>
              </div>
              <div class="product-template__wrapper--content__form--atc">
                  <button class="js-atc f aic jcc w100 bg-titan-green font-sans btn--add-to-cart color-white">
                    <div id="addToCartText-{{ section.id }}">ADD TO CART</div>
                  </button>
                  <div class="amazon-notice"><span>Only Here:</span> Not Available on Amazon or eBay</div>
              </div>
              <div class="product-template__icons">
                  <div class="product-template__icons--item">
                      <img class="product-template__icons--item__icon" src="https://cdn.shopify.com/s/files/1/0071/1727/5191/files/icons8-deliver-food-50_256x256_crop_center.png?v=1677604414" alt="">
                      <span>Free Delivery</span>
                  </div>
                  <div class="product-template__icons--item">
                      <img style="margin-top:0" class="product-template__icons--item__icon" src="https://cdn.shopify.com/s/files/1/0071/1727/5191/files/icons8-ecommerce-warranty-64_256x256_crop_center.png?v=1677604692" alt="">
                      <span>Lifetime Warranty</span>
                  </div>
              </div>   
              <div class="product-template__info">
                  <img class="product-template__info--svg" src="https://cdn.shopify.com/s/files/1/0071/1727/5191/files/icons8-light-on-100_256x256_crop_center.png?v=1677605481" alt="">
                  <div>
                      <p>{{section.settings.idea}}</p>
                  </div>
              </div>    
        </div>
    </div>
  </div>
</section>
{% schema %}
{
  "name": "Product Template",
  "settings": [
    {
      "type": "text",
      "id": "punchline",
      "label": "Punch Line",
      "default": "Finally! A charger that doesn't break."
    },
    {
      "type": "text",
      "id": "d1",
      "label": "Discount message 1",
      "default": "60% OFF + FREE CABLE TODAY!"
    },
    {
      "type": "text",
      "id": "d2",
      "label": "Discount message 2",
      "default": "60% OFF"
    },
    {
      "type": "text",
      "id": "d3",
      "label": "Discount message 3",
      "default": "50% OFF"
    },
    {
      "type": "text",
      "id": "idea",
      "label": "Pro Tip",
      "default": "This cable takes an incredible 1,500 lbs of force to break! Whilst allowing charging up to 2.4x faster!"
    }
  ],
  "blocks": [
    {
        "type": "bullets",
        "name": "Bullet Points",
        "settings": [
          {
            "type": "text",
            "id": "text",
            "label": "Text"
          }
        ]
      }
  ],
  "presets": [
    {
      "name": "Product Template",
      "category": "Home Page"
    }
  ]
}
{% endschema %}
