          {% endif %}

          {% comment %} HIGHLIGHTED UPSELL PRODUCT {% endcomment %}
          {% if product_highlighted %}
            {% liquid
              assign product_highlighted_image = product_highlighted.featured_image
              assign product_highlighted_price = product_highlighted.price
              assign product_highlighted_compare_at_price = product_highlighted.compare_at_price

              assign product_highlighted_title = section.settings.product_highlighted_title
