                  id="pdp-hero__checkbox"
                  class="js-upsell-checkbox"
                  {% if forloop.index == 1 %}
                    checked
                  {% endif %}
                >
                <div class="pdp-hero__extra-checkbox">
                  <svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="25" height="25" viewBox="0,0,256,256">
