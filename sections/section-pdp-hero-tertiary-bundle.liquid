<script>
    {% comment %} document.addEventListener('DOMContentLoaded', function() {
        const pdpHero = document.querySelector('.section-pdp-hero-tertiary')
        const checkboxes = pdpHero.querySelectorAll('.js-upsell-checkbox')

        checkboxes.forEach(checkbox => {
            checkbox.addEventListener('change', function () {
                updateTotalPrice()
            })
        })

        function updateTotalPrice() {
            var mainProductPrice = parseFloat(pdpHero.querySelector('.js-main-product-price').getAttribute('data-price'));
            var mainProductComparePrice = parseFloat(pdpHero.querySelector('.js-price-compare-main-product').getAttribute('data-compare-price'));
            var totalMainProductPrice = mainProductPrice;
            var totalMainProductComparePrice = mainProductComparePrice;
            var priceDif = pdpHero.querySelector('.pdp-hero__price-diff');

            checkboxes.forEach(checkbox => {
                if (checkbox.checked) {
                    var price = parseFloat(checkbox.closest('.pdp-hero__upsell-wrapper').querySelector('.js-upsel-product-price').getAttribute('data-price'));
                    var comparePrice = parseFloat(checkbox.closest('.pdp-hero__upsell-wrapper').querySelector('.js-price-discount').getAttribute('data-compare-price'));

                    totalMainProductPrice += price;
                    totalMainProductComparePrice += comparePrice;
                }
            });

            var totalElement = pdpHero.querySelector('.pdp-hero__pricing-price');
            if (totalElement) {
                totalMainProductPrice /= 100;
                totalElement.textContent = totalMainProductPrice.toLocaleString('en-US', { style: 'currency', currency: '{{shop.currency}}' });
            }

            var comparePriceElement = pdpHero.querySelector('.js-price-compare-main-product');
            if (comparePriceElement) {
                totalMainProductComparePrice /= 100;
                comparePriceElement.textContent = totalMainProductComparePrice.toLocaleString('en-US', { style: 'currency', currency: '{{shop.currency}}' });
            }

            let priceDifCalc = totalMainProductComparePrice - totalMainProductPrice
            priceDif.textContent = `${priceDifCalc.toLocaleString('en-US', { style: 'currency', currency: '{{shop.currency}}' })} SAVED`;
        }

        updateTotalPrice();
    }){% endcomment %}
