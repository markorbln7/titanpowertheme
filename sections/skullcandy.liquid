  }

  .txts { 
    width: 80%;
    place-self: center;
  }

  .iconobud {

    justify-content: flex-start;
    display: grid !important;
    grid-template-columns: .8fr 4fr;
    max-height: 90px;
    min-height: 90px;
    margin: 10px 0;
    align-items: center;
/*     padding-bottom: 50px; */
    width: 80%;
    margin: auto;
    
  }
  .iconobud h3 {
    margin:0;
    padding: 0px;
  }
  {% endfor %} 

  /* DESKTOP ONLY  */
    @media only screen and (min-width: 860px) {
