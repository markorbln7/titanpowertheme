      .stillside {
    margin-top: 200px;
    margin: 10% 10% 0 10%;
       overflow: auto;
  }

  .freedel {
    margin-left: 5px;
  }
.stillside {
     width: 50vw;
  margin: 0;
     }
  .buds-slider-{{section.id}} {
