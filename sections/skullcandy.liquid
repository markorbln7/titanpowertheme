  }
}
/* MOBILE */
  @media only screen and (max-width: 769px) {
     .buds-slider-{{section.id}}{
       display: flex;
       flex-direction: column-reverse;
     }
     .iconobud {
      width: 80%;
      margin: auto;
    } 
    
    #ProductInfo-{{ section.id }}{
        width: 80% !important;
    margin: auto !important;
      margin-top: 10%;
}
        .stillside {
     width: 98vw;
                 overflow: auto;
          
     }
	.swiper {
    max-height: 50vh;
    width: 90%;
    align-self: center;
    margin: auto;
    object-fit: contain;
}

  {% if section.settings.hideonmobile %}
     .stillside {
     width: 98vw;
              overflow: auto;
     }

     .swiper .mySwiper,
     .swiper, .textslide {
       display: none;
     }

    .iconobud {
      margin-left: 10%;
    }
     {% endif %}
  }

</style>
<script>
      var swiper = new Swiper(".mySwiper", {
        loop: true,
        mousewheel: false,
        keyboard: {
        enabled: true,
        onlyInViewport: false,
        grabCursor: true,
        speed: 1000,
        paginationClickable: true,
        parallax: true,
        autoplay: true,
        effect: "slide",
        },
        pagination: {
          el: ".swiper-pagination",
          clickable: true,  
        },
        navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
        },
      });

              document.addEventListener('shopify:section:load', () => {
                  var swiper = new Swiper(".mySwiper", {
       
        loop: true,
        autoplay: {
        delay: 5000,
        },
        keyboard: {
        enabled: true,
        onlyInViewport: false,
        grabCursor: true,
        speed: 1000,
        paginationClickable: true,
        parallax: true,
        autoplay: true,
        effect: "slide",
        },
        pagination: {
          el: ".swiper-pagination",
          clickable: true,  
        },
        navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
        },
      });
          })

</script>
{% schema %}

{
  "name": "buds Page Slider",
  "class": "section",
  "settings": [
    {
      "type": "header",
      "content": "Product",
      "info": "Choose Featured Product"
    }, {
      "type": "product",
      "id": "product",
      "label": "Featured Product"
    }, {
      "type": "header",
      "content": "Layout Options"
    }, {
      "type": "color_background",
      "id": "bg",
      "default": "#FAFAFA",
      "label": "Section/Slider Background"
    }, {
      "type": "image_picker",
      "id": "stillimg",
      "label": "Still Image",
      "info": "Pick the image that will not be a part of the slider - Still Imsge"
    }, {
      "type": "image_picker",
      "id": "sliderimgmobile",
      "label": "Slider Image On Mobile",
      "info": "If 'Hide on Mobile' is selected, leave this blank"
    }, {
      "type": "checkbox",
      "id": "hideonmobile",
      "default": false,
      "label": "Hide Slider  On Mobile",
      "info": "Switch to mobile view to decide if you want to hide this image on mobile"
    }, {
      "type": "color",
      "id": "boxshadow",
      "label": "Arrow Colorr",
      "default": "#FAFAFA"
    }, {
      "type": "range",
      "id": "margin",
      "label": "Top and Bottom Margin",
      "unit": "px",
      "min": 0,
      "max": 100,
      "step": 1,
      "default": 10
    }, {
      "type": "range",
      "id": "shd",
      "label": "Slider Heading Size - Desktop",
